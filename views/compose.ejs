<%- include('partials/header'); -%>

<body>
  <h1>Compose</h1>
  <form action="/compose" method="post">
    <div class="form-group">
      <label>Title</label>
      <input name="postTitle" type="text" class="form-control" autocomplete="off">
      <label>Post</label>
      <textarea name="postBody" class="form-control" rows="5" cols="20"></textarea>
    </div>
    <button type="submit" class="btn btn-primary">Publish</button>
  </form>

</body>
<%- include('partials/footer'); -%>
